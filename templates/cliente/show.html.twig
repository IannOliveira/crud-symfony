
{% extends 'base.html.twig' %}

{% block title %}Cliente{% endblock %}

{% block body %}
    <h1>Cliente</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Cod</th>
                <td>{{ cliente.cod }}</td>
            </tr>
            <tr>
                <th>Nome</th>
                <td>{{ cliente.nome }}</td>
            </tr>
            <tr>
                <th>Nascimento</th>
                <td>{{ cliente.nascimento ? cliente.nascimento|date('d-m-Y') : ''}}</td>
            </tr>
            <tr>
                <th>CPF</th>
                <td>{{ cliente.cpf }}</td>
            </tr>
            <tr>
                <th>Cod Acesso</th>
                <td>{{ cliente.acesso.cod }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ cliente.acesso.email }}</td>
            </tr>
            <tr>
                <th>Senha</th>
                <td>{{ cliente.acesso.password }}</td>
            </tr>
            <tr>
                <th>Cod Endere√ßo</th>
                <td>{{ cliente.endereco.cod }}</td>
            </tr>
            <tr>
                <th>Logradouro</th>
                <td>{{ cliente.endereco.logradouro }}</td>
            </tr>
            <tr>
                <th>Numero Imovel</th>
                <td>{{ cliente.endereco.numeroImovel }}</td>
            </tr>
            <tr>
                <th>Complemento</th>
                <td>{{ cliente.endereco.complemento }}</td>
            </tr>
            <tr>
                <th>Bairro</th>
                <td>{{ cliente.endereco.bairro }}</td>
            </tr>
            <tr>
                <th>Cidade</th>
                <td>{{ cliente.endereco.cidade }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ cliente.endereco.estado }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ cliente.endereco.latitude }}</td>
            </tr>
            <tr>
                <th>Longitude</th>
                <td>{{ cliente.endereco.longitude }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('cliente_index') }}">Voltar</a>
{% endblock %}
